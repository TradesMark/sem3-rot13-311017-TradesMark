from urllib.request import urlopen
import json
import pprint

print('Введите id:')
id = input()
hacker_man = "https://api.vk.com/method/users.get?user_ids={id}&fields=education&v=5.69".format(id = id)
request_obj = urlopen(hacker_man)

obj = json.loads(request_obj.read())
print(hacker_man)
print('')
pprint.pprint(obj)
